<div class="container">
  <% if session_logged_in? %>
    <form action='/tweets' method='post' id="tweet_box">
      <input type='text' name='content' placeholder="What's Cattening?">
      <input type='submit' value='Meow!'>
    </form>
  <% end %>
  <% @tweets.each do |tweet| %>
    <table id="tweet_feed">
      <tr>
        <td><b><%= tweet.user.username %></b></td>
      </tr>
      <tr>
        <td><p id="tweet_content"><%= tweet.content %></p></td>
      </tr>
    </table>
    <br>
  <% end %>
</div>


