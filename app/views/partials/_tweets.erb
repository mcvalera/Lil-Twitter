<h1>Your Tweets</h1>
  <% filter_tweets_for_user.each do |tweet| %>
    <table id="tweet_feed">
      <tr>
        <td><b><%= tweet.user.username %></b></td>
      </tr>
      <tr>
        <td><p id="tweet_content"><%= tweet.content %></p></td>
      </tr>
      <tr>
        <td><a href="/tweets/retweet/<%= tweet.id %>">re-meow!</a></td>
      </tr>
      <tr>
        <td><% if tweet.retweeted? %>
          <p>re-meowed from <%= tweet.original_user.username %></p>
          <% end %>
        </td>
      </tr>
    </table>
    <br>
  <% end %>
</div>
